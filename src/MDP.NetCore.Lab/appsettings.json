{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.*": "Warning",
      "System.*": "Warning"
    }
  },

  "MyLab.Modules": {
    "HelloWorkService": {
      "Message": "Hello World 123"
    },
    "HelloWorkService[456]": {
      "Message": "Hello World 456"
    },
    "DecorateWorkService[AAA]": {
      "Message": "AAA",
      "WorkService": "HelloWorkService[456]"
    },
    "DecorateWorkService[BBB]": {
      "Message": "BBB",
      "WorkService": "DecorateWorkService[AAA]"
    },
    "DecorateWorkService[CCC]": {
      "Message": "CCC",
      "WorkService": "DecorateWorkService[BBB]"
    }
  }
}