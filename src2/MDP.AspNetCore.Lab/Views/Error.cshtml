@using MDP.AspNetCore;
@model ProblemDetails

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <title></title>
</head>
<body>

    <!--Title-->
    <h2>MDP.AspNetCore.Lab</h2>
    <hr />

    <!--Message-->
    <h3>TraceId: @Model.GetTraceId()</h3>    
