{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.*": "Warning",
      "System.*": "Warning"
    }
  },

  "WebApplication1": {

    // Typed
    "DecorateDemoService": {
      "Message": "Decorator2",
      "component": "DecorateDemoService[A]"
    },

    // Decorate
    "DecorateDemoService[A]": {
      "Message": "Decorator1",
      "component": "ConcreteDemoService[B]"
    },

    // Concrete
    "ConcreteDemoService[B]": {
      "Message": "Hello World"
    }
  }
}